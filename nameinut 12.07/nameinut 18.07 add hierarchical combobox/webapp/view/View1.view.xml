<mvc:View xmlns:html="http://www.w3.org/1999/xhtml"
xmlns:mvc="sap.ui.core.mvc" 
xmlns:core="sap.ui.core" 
xmlns="sap.m" 
controllerName="nameInput.controller.View1" 
displayBlock="true"
xmlns:l="sap.ui.layout">
	<App id="app">
		<pages>
			<Page title="{i18n>title}" >
				<content>
					<Input id="name" type="Text" placeholder="Enter your Name" submit="checkInput"/>
				
				<l:VerticalLayout>
					<Button id="but" text="Send" type="Accept" press="onPress"/>
					<Button id="but2" text="Next page" press="page2"/>
					<Button id="but3" text="go to page 3" press="page3"/>
					<Button text="go to page 4" press="page4"></Button>
					<Button text="go to page 5 list" press="page5"></Button>
					<Button text="go to page 6" press="page6"></Button>
					<Text text="BUHLO CHECKER"></Text>
					<Input id="ageinput" width="100%" type="Text" placeholder="..."></Input>
					<Button id="agecheck" text="check" press="check"></Button>
				
				</l:VerticalLayout>
				    <!--<ToggleButton text="Button" width="100px" id="__button1"/>-->
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>