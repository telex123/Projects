{
	"_version": "1.7.0",
	"sap.app": {
		"id": "nameInput",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"northwind": {
				"uri": "/northwind/V2/northwind/northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "nameInput.view.View1",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "nameInput.i18n.i18n"
				}
			},
			"": {
				"dataSource": "northwind",
				"settings": {
					"json": true,
					"useBatch": false
				}
			},

			"users": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/users.json"
			}
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "nameInput.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide"
			},
			"routes": [{
				"pattern": "view1",
				"name": "view1",
				"target": "view1"
			}, {
				"pattern": "view2/:invoicePath:/:invo:",
				"name": "view2",
				"target": "view2"
			}, {
				"pattern": "employeeList",
				"name": "employeeList",
				"target": "employeeList"
			}, {
				"pattern": "view3",
				"name": "view3",
				"target": "view3"
			},
			{
				"pattern": "view4",
				"name": "view4",
				"target": "view4"
			},
			{
				"pattern": "view5",
				"name": "view5",
				"target": "view5"
			}],
			"targets": {
				"view1": {
					"viewName": "View1"
				},
				"view2": {
					"viewName": "View2"
				},
				"EmployeeList": {
					"viewId": "employeeList",
					"viewName": "EmployeeList",
					"viewLevel": 2
				},
				"view3": {
					"viewName": "View3"
				},
				"view4": {
					"viewName": "View4"
				},
				"view5": {
					"viewName": "View5"
				}
			},
			"resources": {
				"css": [{
					"uri": "css/style.css"
				}]
			}
		}
	}
}